{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/livewire-starter-kit",
    "type": "project",
    "description": "The official Laravel starter kit for Livewire.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0",
        "laravel/horizon": "^5.30.3",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.10.1",
        "livewire/livewire": "^3.6",
        "livewire/volt": "^1.7",
        "owenvoke/blade-fontawesome": "^2.9",
        "pgvector/pgvector": "^0.2.2",
        "pragmarx/version": "dev-master",
        "predis/predis": "^3.2",
        "robsontenorio/mary": "^2.4",
        "sentry/sentry-laravel": "^4.9",
        "socialiteproviders/authelia": "^4.0",
        "spatie/laravel-activitylog": "^4",
        "spatie/laravel-tags": "^4.10",
        "willscottuk/monzo-php": "dev-master"
    },
    "require-dev": {
        "deployer/deployer": "^7.5",
        "fakerphp/faker": "^1.23",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.18",
        "laravel/sail": "^1.44",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "phpunit/phpunit": "^12.0.0",
        "ryoluo/sail-ssl": "^1.4",
        "tightenco/duster": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan sentry:publish --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan horizon\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,horizon,logs,vite --kill-others"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        },
        "platform": {
            "php": "8.2"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:cronxco/graby.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:willscottuk/monzo-php.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:cronxco/plaid-sdk-php.git"
        },
        {
            "type": "vcs",
            "url": "git@github.com:cronxco/version.git"
        },
        {
            "name": "wire-elements/pro",
            "type": "composer",
            "url": "https://wire-elements-pro.composer.sh",
            "only": [
                "wire-elements/*"
            ]
        },
        {
            "type": "vcs",
            "url": "git@github.com:willscottuk/laravel-supabase-auth.git"
        }
    ]
}
